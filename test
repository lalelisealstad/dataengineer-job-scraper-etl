import pandas as pd
import gcsfs
import os

# Define the path to your GCS bucket and file
file_path_gcp = "gs://oslo-linkedin-dataengineer-jobs/transformed/jobs_06082024.parquet"
os.environ["GOOGLE_APPLICATION_CREDENTIALS"] = r"../service-account-details.json" 


# Initialize the GCS file system
fs = gcsfs.GCSFileSystem()

# Read the Parquet file from GCS
with fs.open(file_path_gcp, 'rb') as f:
    df = pd.read_parquet(f)

# Display the DataFrame
print(df)